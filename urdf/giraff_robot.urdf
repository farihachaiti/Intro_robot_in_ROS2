<?xml version="1.0"?>
<robot name="giraff_robot">
    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>
    <material name="blue">
        <color rgba="0 0 1 1"/>
    </material>


    <world name="world">
	 <origin rpy="0 0 0" xyz="0 0 0"/>
    </world>


    <link name="base_link">
        <visual> 
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
            <material name="red"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.1 0.1 0.1"/>
            </geometry>
            <origin xyz="0 0 0"/>
        </collision>
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
        </inertial>
    </link>
    <link name="leg_link">
        <visual>
            <geometry>
                <cylinder length="0.6" radius="0.5"/>
            </geometry>
            <material name="red"/>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.6" radius="0.5"/>
            </geometry>
            <origin xyz="0 0 0"/>
        </collision>
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
        </inertial>
    </link> 
    <link name="arm_link">
        <visual>
            <geometry>
                <cylinder length="0.6" radius="0.5"/>
            </geometry>
            <material name="red"/>
        </visual> 
        <collision>
            <geometry>
                <cylinder length="0.6" radius="0.5"/>
            </geometry>
            <origin xyz="0 0 0"/>
        </collision>
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
        </inertial>
    </link>
    <link name="end_effector">
        <visual>
            <geometry>
                <cylinder length="0.6" radius="0.5"/>
            </geometry>
            <material name="red"/>
        </visual> 
        <collision>
            <geometry>
                <cylinder length="0.6" radius="0.5"/>
            </geometry>
            <origin xyz="0 0 0"/>
        </collision>
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
        </inertial>
    </link>
    <link name="tool_mic">
        <visual>
            <geometry>
                <cylinder length="0.6" radius="0.5"/>
            </geometry>
            <material name="red"/>
        </visual> 
        <collision>
            <geometry>
                <cylinder length="0.6" radius="0.5"/>
            </geometry>
            <origin xyz="0 0 0"/>
        </collision>
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
        </inertial>
    </link>


    <joint name="base_joint" type="revolute">
        <parent link="world"/>
        <child link="base_link"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <axis xyz="0 0 1" />
        <safety_controller k_position="3.1416" k_velocity="1.0" k_effort="100.0"/>
        <calibration rising="0.0" falling="0.0"/>
        <dynamics damping="0.0" friction="0.0"/>
        <limit lower="-3.1416" upper="3.1416" effort="100.0" velocity="1.0" />
        <visual>
            <geometry>
                <sphere radius="0.5"/>
            </geometry>
            <material name="blue"/>
        </visual> 
    </joint>
    <joint name="base_spherical_joint" type="revolute">
        <parent name="base_link"/>
        <child link="leg_link"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="0 0 1" />
        <safety_controller k_position="3.1416" k_velocity="1.0" k_effort="100.0"/>
        <calibration rising="0.0" falling="0.0"/>
        <dynamics damping="0.0" friction="0.0"/>
        <limit lower="-3.1416" upper="3.1416" effort="100.0" velocity="1.0" />
        <visual>
            <geometry>
                <sphere radius="0.5"/>
            </geometry>
            <material name="blue"/>
        </visual> 
    </joint>
    <joint name="leg_joint" type="prismatic">
        <parent link="leg_link"/>
        <child link="arm_link"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <limit lower="0.0" upper="100.0" effort="100.0" velocity="1.0" />
        <safety_controller k_position="100.0" k_velocity="1.0" k_effort="100.0"/>
        <calibration rising="0.0" falling="0.0"/>
        <dynamics damping="0.1" friction="0.0"/>
        <visual>
            <geometry>
                <box size="0.1 0.2 0.6"/>
            </geometry>
            <material name="blue"/>
        </visual> 
    </joint>
    <joint name="arm_joint" type="revolute">
        <parent link="arm_link"/>
        <child link="end_effector"/>
        <axis xyz="0 0 1" />
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <safety_controller k_position="3.1416" k_velocity="1.0" k_effort="100.0"/>
        <calibration rising="0.0" falling="0.0"/>
        <dynamics damping="0.0" friction="0.0"/>
        <limit lower="-3.1416" upper="3.1416" effort="100.0" velocity="1.0" />
        <visual>
            <geometry>
                <sphere radius="0.5"/>
            </geometry>
            <material name="blue"/>
        </visual> 
    </joint>
    <joint name="end_effector_joint" type="revolute">
        <parent link="end_effector"/>
        <child link="tool_mic"/>
        <axis xyz="0 0 1" />
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <safety_controller k_position="3.1416" k_velocity="1.0" k_effort="100.0"/>
        <calibration rising="0.0" falling="0.0"/>
        <dynamics damping="0.0" friction="0.0"/>
        <limit lower="0.0" upper="30.0" effort="100.0" velocity="1.0" />
        <stiffness value="0.0"/>
        <visual>
            <geometry>
                <box size="0.6 0.1 0.2"/>
            </geometry>
            <material name="blue"/>
        </visual> 
    </joint>
</robot>

